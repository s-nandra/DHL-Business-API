            <xs:documentation>begin of timeframe</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="until" type="xs:time">
          <xs:annotation>
            <xs:documentation>end of timeframe</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
